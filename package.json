{
	"name": "wil-astro-starter",
	"type": "module",
	"version": "0.0.1",
	"imports": {
		"#template/*": "./astro-template/astro/*",
		"#template-root/*": "./astro-template/*"
	},
	"scripts": {
		"dev": "npm run update && npx tinacms dev -c \"astro dev --port 8080\"",
		"start": "npm run update && npx tinacms dev -c \"astro dev --port 8080\"",
		"build": "npm run update && npx tinacms build && astro build",
		"preview": "astro preview --port 8080",
		"update": "cp -R ./astro-template/update-files/* ./"
	},
	"dependencies": {
		"@astrojs/mdx": "^1.1.3",
		"@astrojs/react": "^3.0.4",
		"@astrojs/tailwind": "^5.0.2",
		"@astrojs/sitemap": "^3.0.4",
		"@tinacms/cli": "^1.5.42",
		"@types/react": "^18.2.33",
		"@types/react-dom": "^18.2.14",
		"astro-icon": "^1.0.2",
		"astro": "^3.4.0",
		"fs-extra": "^11.1.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tailwindcss": "^3.3.5",
		"tinacms": "^1.6.0",
		"tailwind-fontawesome": "^0.6.0"
	},
	"devDependencies": {
		"@types/node": "^20.8.9",
		"prettier": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.6"
	}
}